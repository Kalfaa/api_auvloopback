{
  "name": "profile",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "old": {
      "type": "boolean",
      "required": true
    },
    "fname": {
      "type": "string",
      "required": true
    },
    "lname": {
      "type": "string",
      "required": true
    },
    "birthdate": {
      "type": "date",
      "required": true
    },
    "photoid": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "location": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "rejected": {
      "type": "hasMany",
      "model": "profile",
      "as": "rejected",
      "foreignKey": "oldId",
      "through": "rejected",
      "keyThrough": "newId"
    },
    "accepted": {
      "type": "hasMany",
      "model": "profile",
      "as": "accepted",
      "foreignKey": "newId",
      "through": "accepted",
      "keyThrough": "oldId"
  },
    "messages":{
        "type": "hasMany",
        "model": "profile",
        "as": "messages",
        "foreignKey": "sender",
        "through": "messages",
        "keyThrough": "receiver"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
